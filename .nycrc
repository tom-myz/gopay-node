{
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register/type-check"
  ],
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html"
  ],
  "sourceMap": false,
  "cwd": "src",
  "all": true,
  "check-coverage": true,
  "per-file": false,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 0
}
